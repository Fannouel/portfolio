{
  "rewrites": [
    { "source": "/", "destination": "/fr/index.html" },
    { "source": "/fr", "destination": "/fr/index.html" },
    { "source": "/fr/**", "destination": "/fr/$1" },
    { "source": "/en", "destination": "/en/index.html" },
    { "source": "/en/**", "destination": "/en/$1" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
